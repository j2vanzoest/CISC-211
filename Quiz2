section .text
                global _start

_start:
                mov cl,[c]
                mov bl,[d]
                mul bl

                mov al,[a]
                mov dl,[b]
                mul dl

                add al,cl
                mov [x],eax

                mov eax,1
                int 0x80

section .data
                a DD 2;
                b DD 3;
                c DD 4;
                d DD 1;
segment .bss
                x resb 1
