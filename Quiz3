section .text
        global _start
_start:
        mov eax,1
        mov ecx, [var]
        mov eax, ecx
factorial:
        cmp ecx, 1
        jle exit

        sub ecx, 1
        mul ecx
        jmp factorial
exit: 
         mov [result],eax

         mov eax,1
        int 0x80

section .data
        var DD 5
segment .bss
    result resb 1
